# Uncomment this line to define a global platform for your project 
platform :ios, '13.0'

# CocoaPods analytics sends network stats
ENV['COCOAPODS_DISABLE_STATS'] = 'true'

project 'Runner',
  'Debug' => :debug,
  'Profile' => :release,
  'Release' => :release

# Load environment variables (generated by Flutter)
# (Comment if your project doesn't generate this)
# require File.expand_path('flutter_export_environment.rb', __dir__)

# Load the Flutter Pod helper (KEEP this so we can use flutter_install_all_pods!)
require File.expand_path(File.join('..', 'Flutter', 'podhelper.rb'), __dir__)

target 'Runner' do
  use_frameworks!
  
  flutter_install_all_pods File.dirname(File.realpath(__FILE__))

  # If you manually add pods, put them below:
  # pod 'Firebase/Core'
  # pod 'Firebase/Firestore'
end
